{
  "name": "crud-react-redux",
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "sourceMaps": "inline",
    "retainLines": true
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "debug": "~2.6.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-plugin-react": "^7.10.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "morgan": "~1.9.0",
    "nyc": "^12.0.2",
    "pug": "^2.0.0-beta11",
    "react-test-renderer": "^16.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  },
  "scripts": {
    "start": "babel-node index.js",
    "pack": "webpack --config webpack.config.js",
    "babel": "babel src -d build",
    "build": "npm run pack && npm run babel",
    "debug": "set DEBUG=crud-react-redux:* & npm start",
    "eslint": "eslint src test index.js --ext js,jsx",
    "eslint:fix": "eslint src test index.js --ext js,jsx --fix",
    "testwithoutreport": "mocha \"test/**/*.spec.js\" --compilers js:babel-core/register  --require test/test_helper.js",
    "test": "cross-env NODE_ENV=test nyc --extension .jsx --extension .es6 --reporter=lcov --reporter=text --exclude \"test/**/*\"  mocha \"test/**/*.spec.js\" -t 10000 --require test/test_helper.js --compilers js:babel-core/register"
  }
}
