{
  "name": "crud-react-redux",
  "sideEffects": false,
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "comments": true,
    "env": {
      "test": {
        "presets": [
          "env",
          "stage-2"
        ],
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ],
        "retainLines": true
      }
    }
  },
  "dependencies": {
    "compression": "^1.7.3",
    "connected-react-router": "^4.4.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "http-errors": "^1.7.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "pg": "^7.4.3",
    "pug": "^2.0.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "nodemon-webpack-plugin": "^4.0.3",
    "nyc": "^12.0.2",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.5",
    "style-loader": "^0.21.0",
    "treer": "^1.0.4",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "start": "webpack --watch --config webpack.dev.js --trace-sync-io",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint src test --ext js,jsx",
    "lint:fix": "eslint src test --ext js,jsx --fix",
    "testwithoutreport": "cross-env NODE_ENV=test mocha \"test/**/*.spec.js\" --require babel-register --require test/test_helper.js",
    "test": "cross-env NODE_ENV=test nyc --extension .jsx --reporter=lcov --reporter=text mocha \"test/**/*.spec.js\" -t 10000 --require babel-register --require test/test_helper.js",
    "heroku-postbuild": "npm run build",
    "treer": "treer -i \"/^.git|node_modules|.nyc_output|.vscode|coverage|dist|crud.log.gz|treerOutput$/\" -e treerOutput"
  }
}
