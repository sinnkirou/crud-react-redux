{
  "name": "crud-react-redux",
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "comments": true,
    "env": {
      "test": {
        "presets": [
          "env",
          "stage-2"
        ],
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ],
        "retainLines": true
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.3",
    "connected-react-router": "^4.4.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "debug": "~2.6.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-plugin-react": "^7.10.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "nodemon-webpack-plugin": "^4.0.3",
    "nyc": "^12.0.2",
    "pug": "^2.0.0-beta11",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.3.3",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.5",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.0.0"
  },
  "scripts": {
    "start": "webpack --watch --config webpack.dev.js --trace-sync-io",
    "build": "webpack --config webpack.prod.js",
    "debug": "set DEBUG=crud-react-redux:* && npm start",
    "eslint": "eslint src test --ext js,jsx",
    "eslint:fix": "eslint src test --ext js,jsx --fix",
    "testwithoutreport": "cross-env NODE_ENV=test mocha \"test/**/*.spec.js\" --require babel-register --require test/test_helper.js",
    "test": "cross-env NODE_ENV=test nyc --extension .jsx --reporter=lcov --reporter=text mocha \"test/**/*.spec.js\" -t 10000 --require babel-register --require test/test_helper.js"
  }
}
